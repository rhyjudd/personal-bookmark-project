<!doctype html>
<html lang="en">
  <head>
    <%- include("../partials/head.ejs") %>
  </head>
  <body>
    <header class="sticky-top">
    <%- include("../partials/header.ejs") %>
    </header>
    <main class="container" >
    <div>
       <% bookmarks.forEach((bookmarks) => {
      %> 
      <a href="<%= bookmarks._id %>" class="bg-secondary text-light">  
        <div class="text-light bookmarkbox ">
          <%= bookmarks.url %>    
        </div> 
      </a>
        
        <%
        }) %>   
    </div>
    </main>
    <footer>
      <%- include("../partials/footer.ejs") %>
    </footer>  
  </body>
</html>